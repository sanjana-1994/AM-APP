			device = "IOS";
		}else{
			device = "unknown";
			console.log('Unknown Mobile Device...');
		}
	}else{
		if( client.isJava() ) { // Check If Java Is Installed
			javaVersion = client.getJavaVersion();
  			console.log('Java ' + client.getJavaVersion() ); // Get Java Version
		}else{
			javaVersion = "No Java"
			console.log('No Java Installed...');
